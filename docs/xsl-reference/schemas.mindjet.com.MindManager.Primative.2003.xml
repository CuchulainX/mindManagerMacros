<xsd:schema targetNamespace="http://schemas.mindjet.com/MindManager/Primitive/2003" elementFormDefault="qualified" attributeFormDefault="unqualified" version="16.1.0.2-r" xmlns:pri="http://schemas.mindjet.com/MindManager/Primitive/2003" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<xsd:annotation>
		<xsd:documentation>MindManager primitive types application schema</xsd:documentation>
	</xsd:annotation>
	<xsd:simpleType name="Color">
		<xsd:annotation>
			<xsd:documentation>ARGB value of color; 4 bytes, 1 byte per channel; A - alpha, R - red, G - green, B - blue</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:hexBinary">
			<xsd:length value="4" fixed="false" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Unit">
		<xsd:annotation>
			<xsd:documentation>Dimension unit as signed floating point number in millimeters; is a base of dimension units</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:float" />
	</xsd:simpleType>
	<xsd:simpleType name="Offset">
		<xsd:annotation>
			<xsd:documentation>Signed offset unit</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="pri:Unit" />
	</xsd:simpleType>
	<xsd:simpleType name="Size">
		<xsd:annotation>
			<xsd:documentation>Unsigned size unit</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="pri:Unit">
			<xsd:minInclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ZoomFactor">
		<xsd:annotation>
			<xsd:documentation>Zooming factor as unsigned floating point; where 1.0 is 100%</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:float">
			<xsd:minExclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="SpacingStretchFactor">
		<xsd:annotation>
			<xsd:documentation>Spacing stretch factor as unsigned floating point; where 1.0 is 100%</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:float">
			<xsd:minExclusive value="0" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Level">
		<xsd:annotation>
			<xsd:documentation>Level within document's tree structure; where 0 is root topic, 1 are topics contained by root topic etc.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:unsignedInt" />
	</xsd:simpleType>
	<xsd:simpleType name="TextAlignment">
		<xsd:annotation>
			<xsd:documentation>Text aligments</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:anyURI">
